# Mobile Banking Infrastructure - Environment Variables
# Copy this file to .env and fill in the values

# PostgreSQL Auth Database
POSTGRES_USER=postgres
POSTGRES_AUTH_PASSWORD=your-secure-auth-db-password

# PostgreSQL User Database
POSTGRES_USER_PASSWORD=your-secure-user-db-password

# JWT Secret (minimum 32 characters)
JWT_SECRET=your-256-bit-secret-key-for-jwt-token-signing-minimum-32-chars

# Grafana Admin
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your-secure-grafana-password
